{
    "name": "seti-classic-vscode",
    "type": "dark",
    "colors": {
        /*
		Main Colors:
		Blue: 	#55b5db
		Green: 	#9fca56
		Yellow: #e6cd69
		Red: 	#cd3f45
		Purple: #a074c4
		*/
        // Editor
        "editor.background": "#191B1C",
        "editor.foreground": "#d4d7d6",
        "editorLineNumber.foreground": "#41535b",
        "editorLineNumber.activeForeground": "#55b5db",
        "editor.wordHighlightBackground": "#356577",
        "editorRuler.foreground": "#1d1f21",
        "editorError.foreground": "#cd3f45",
        "editorWarning.foreground": "#e6cd69",
        "editorInfo.foreground": "#55b5db",
        "editorHint.foreground": "#a074c4",
        // Gutter
        "editorGutter.background": "#151718",
        "editorGutter.modifiedBackground": "#55b5db",
        "editorGutter.addedBackground": "#9fca56",
        "editorGutter.deletedBackground": "#cd3f45",
        // Activity Bar
        "activityBar.background": "#1D1F21",
        "activityBar.foreground": "#d4d7d6",
        "activityBarBadge.background": "#55b5db",
        // Lists and Trees
        "list.warningForeground": "#e6cd69",
        "list.errorForeground": "#cd3f45",
        "list.invalidItemForeground": "#a074c4",
        // Menu
        "menu.background": "#1D1F21",
        "menu.selectionBackground": "#356577",
        // Title Bar
        "titleBar.activeBackground": "#1D1F21",
        // SideBar
        "sideBar.background": "#131415",
        "sideBarTitle.foreground": "#bbbbbb",
        "sideBar.border": "#1D1F21",
        // Status Bar
        "statusBar.background": "#1D1F21",
        "statusBar.noFolderBackground": "#1D1F21",
        "statusBar.debuggingBackground": "#2b0000",
        // Tabs
        "editorGroup.border": "#111313",
        "editorGroupHeader.tabsBorder": "#111313",
        "editorGroupHeader.tabsBackground": "#22272B",
        "tab.activeBackground": "#111313",
        "tab.unfocusedActiveBackground": "#111313",
        "tab.border": "#111313",
        "tab.activeBorder": "#55b5db",
        "tab.unfocusedActiveBorder": "#55b5db",
        "tab.inactiveBackground": "#1C2123",
        "tab.inactiveForeground": "#d4d7d6",
        // Git
        "gitDecoration.addedResourceForeground": "#9fca56",
        "gitDecoration.modifiedResourceForeground": "#55b5db",
        "gitDecoration.deletedResourceForeground": "#cd3f45",
        "gitDecoration.untrackedResourceForeground": "#a074c4",
        "gitDecoration.ignoredResourceForeground": "#41535b",
        "gitDecoration.conflictingResourceForeground": "#e6cd69",
        "gitDecoration.submoduleResourceForeground": "#356577",
        // Overview Ruler
        "editorOverviewRuler.bracketMatchForeground": "#d4d7d6",
        "editorOverviewRuler.modifiedForeground": "#55b5db",
        "editorOverviewRuler.addedForeground": "#9fca56",
        "editorOverviewRuler.deletedForeground": "#cd3f45",
        "editorOverviewRuler.errorForeground": "#cd3f45",
        "editorOverviewRuler.warningForeground": "#e6cd69",
        "editorOverviewRuler.infoForeground": "#55b5db",
        "editorOverviewRuler.findMatchForeground": "#d4d7d6",
        // Terminal Colors
        // From base16-seti-ui.minttyrc
        "terminal.background": "#151718",
        "terminal.foreground": "#d4d7d6",
        "terminal.ansiBlack": "#151718",
        "terminal.ansiBrightBlack": "#41535b",
        "terminal.ansiBlue": "#55b5db",
        "terminal.ansiBrightBlue": "#88e8ff",
        "terminal.ansiCyan": "#55dbbe",
        "terminal.ansiBrightCyan": "#88fff1",
        "terminal.ansiGreen": "#9fca56",
        "terminal.ansiBrightGreen": "#d2fd89",
        "terminal.ansiYellow": "#e6cd69",
        "terminal.ansiBrightYellow": "#ffff9c",
        "terminal.ansiMagenta": "#a074c4",
        "terminal.ansiBrightMagenta": "#d3a7f7",
        "terminal.ansiWhite": "#d6d6d6",
        "terminal.ansiBrightWhite": "#ffffff",
        "terminal.ansiRed": "#cd3f45",
        "terminal.ansiBrightRed": "#ff7278",
    },
    "tokenColors": [
        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.definition.comment",
                "markup.quote",
                "markup.heading.setext",
                "meta.separator"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#41535b"
            }
        },
        {
            "name": "Blue",
            "scope": [
                "entity.name.function",
                "entity.name.tag.html",
                "support.function",
                "meta.function-call",
                "punctuation.definition.array.begin.json",
                "punctuation.definition.array.end.json",
                "string.quoted.docstring.single",
                "string.quoted.docstring.multi",
                "string.quoted.single",
                "string.quoted.double",
                "support.type.property-name",
                "variable.parameter.function-call",
                "meta.brace.square",
                "entity.name.tag.js.jsx",
                "punctuation.definition.tag.begin.js.jsx",
                "punctuation.definition.tag.end.js.jsx",
                "entity.name.tag.localname.xml",
                "punctuation.definition.tag.xml",
                "punctuation.definition.tag.begin.html",
                "punctuation.definition.tag.end.html",
                "heading",
                "punctuation.definition.metadata",
                "punctuation.definition.string",
                "entity.name.type.namespace.cs"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#55b5db"
            }
        },
        {
            "name": "Green",
            "scope": [
                "variable.other.object.property",
                "entity.name.tag",
                "entity.other.attribute-name.class",
                "entity.other.attribute-name.id",
                "entity.other.attribute-name.pseudo-class",
                "entity.other.inherited-class",
                "keyword.control.at-rule.import",
                "keyword.control.at-rule.media",
                "keyword.operator.gradient.css",
                "keyword.operator.logical.and.media",
                "entity.other.attribute-name.html",
                "keyword.other.important",
                "keyword.other.unit.px",
                "keyword.operator.new",
                "variable.other.property",
                "entity.other.attribute-name.js.jsx",
                "entity.other.attribute-name.localname.xml",
                "entity.other.attribute-name.xml",
                "markup.list.unnumbered",
                "markup.list.numbered",
                "variable.language.special.self",
                "variable.parameter.function.language.special.self.python",
                "meta.preprocessor.cs",
                "storage.type.class.gtkdoc",
                "storage.type.class.doxygen"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#9fca56"
            }
        },
        {
            "name": "Darkened Green",
            "scope": [
                "punctuation.parenthesis.open",
                "punctuation.parenthesis.close",
                "punctuation.definition.typeparameters.begin",
                "punctuation.definition.typeparameters.end"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#9fca56CC"
            }
        },
        {
            "name": "Yellow",
            "scope": [
                "keyword.control",
                "keyword.operator",
                "storage.type.class",
                "storage.type.function",
                "storage.type",
                "punctuation.separator.key-value.html",
                "keyword.codetag.notation",
                "meta.tag.xml",
                "meta.tag.preprocessor.xml",
                "markup.underline.link",
                "punctuation.definition.list",
                "punctuation.separator.array.json.comments",
                "punctuation.separator.dictionary.key-value.json.comments",
                "storage.modifier",
                "keyword.other.class",
                "keyword.other.using",
                "keyword.other.var.cs",
                "keyword.other.namespace.cs",
                "keyword.other.struct.cs",
                "keyword.other.new",
                "punctuation.separator.pointer-access"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#e6cd69"
            }
        },
        {
            "name": "Red",
            "scope": [
                "constant.character.escape",
                "constant.language",
                "constant.numeric",
                "support.constant.font-name",
                "support.constant.media",
                "support.constant.property-value.css"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#cd3f45"
            }
        },
        {
            "name": "Purple",
            "scope": [
                "entity.name.type.class",
                "support.type.property-name.css",
                "support.type.property-name.media",
                "variable.other.object",
                "punctuation.section.embedded.begin.js.jsx",
                "punctuation.section.embedded.end.js.jsx",
                "entity.name.tag.xml",
                "markup.inline.raw.string",
                "string.other.link.title",
                "keyword.type",
                "entity.name.type.struct.cs"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#a074c4"
            }
        },
        {
            "name": "Bold Blue",
            "scope": [
                "punctuation.definition.interpolation.begin",
                "punctuation.definition.interpolation.end"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#88e8ff"
            }
        },
        {
            "name": "Font Color",
            "scope": [
                "variable.language",
                "meta.function-call.arguments",
                "punctuation.definition.arguments.begin",
                "punctuation.definition.arguments.end",
                "string.quoted.double.json",
                "string.quoted.double.html",
                "punctuation.accessor.js.jsx",
                "string.quoted.double.xml",
                "punctuation.definition.string.begin.json.comments",
                "punctuation.definition.string.end.json.comments",
                "entity.name.variable.preprocessor.symbol.cs",
                "meta.block.c"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#d4d7d6"
            }
        },
        {
            "name": "Darkened Font Color",
            "scope": [
                "punctuation.definition.entity",
                "punctuation.terminator.rule",
                "punctuation.terminator.statement.cs"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#888B8A"
            }
        },
        {
            "name": "Bold Font Color",
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#d4d7d6"
            }
        },
        {
            "name": "Italic Font Color",
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#d4d7d6"
            }
        }
    ],
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "namespace": {
            "foreground": "#55b5db",
            "fontStyle": "bold"
        },
        "comment": "#41535B",
        "xmlDocCommentComment": "#41535B",
        "xmlDocCommentAttributeName": "#9fca56",
        "struct": "#a074c4CC",
        "class": "#a074c4",
        "interface": {
            "foreground": "#a074c4",
            "fontStyle": "bold italic"
        },
        "method": "#55b5db",
        "string": "#55b5db",
        "number": "#cd3f45",
        "macro": "#cd3f45",
        "operator": "#e6cd69",
        "operatorOverloaded": {
            "foreground": "#e6cd69",
            "fontStyle": "bold"
        },
        "memberOperatorOverload": {
            "foreground": "#e6cd69",
            "fontStyle": "bold"
        },
        "controlKeyword": "#e6cd69",
        // "plainKeyword": "#e6cd69",
        "local": "#d4d7d6",
        "variable": "#d4d7d6",
        "field": "#d4d7d6",
        "parameter": "#d4d7d6"
    }
}
