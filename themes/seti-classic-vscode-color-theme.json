{
  "name": "seti-classic-vscode",
  "type": "dark",
  "colors": {
    /*
      Main Colors:
      Blue: 	#55b5db
      Green: 	#9fca56
      Yellow: #e6cd69
      Red: 	  #cd3f45
      Purple: #a074c4
    */
    // Editor
    "editor.background": "#191B1C",
    "editor.foreground": "#d4d7d6",
    "editorLineNumber.foreground": "#41535b",
    "editorLineNumber.activeForeground": "#55b5db",
    "editor.wordHighlightBackground": "#356577",
    "editor.foldBackground": "#41535b25",
    "editorRuler.foreground": "#1d1f21",
    "editorError.foreground": "#cd3f45",
    "editorWarning.foreground": "#e6cd69",
    "editorInfo.foreground": "#55b5db",
    "editorHint.foreground": "#a074c4",
    // Gutter
    "editorGutter.background": "#151718",
    "editorGutter.modifiedBackground": "#55b5db",
    "editorGutter.addedBackground": "#9fca56",
    "editorGutter.deletedBackground": "#cd3f45",
    // Activity Bar
    "activityBar.background": "#1D1F21",
    "activityBar.foreground": "#d4d7d6",
    "activityBarBadge.background": "#55b5db",
    // Lists and Trees
    "list.warningForeground": "#e6cd69",
    "list.errorForeground": "#cd3f45",
    "list.invalidItemForeground": "#a074c4",
    // Menu
    "menu.background": "#1D1F21",
    "menu.selectionBackground": "#356577",
    // Title Bar
    "titleBar.activeBackground": "#1D1F21",
    // SideBar
    "sideBar.background": "#131415",
    "sideBarTitle.foreground": "#bbbbbb",
    "sideBar.border": "#1D1F21",
    // Status Bar
    "statusBar.background": "#1D1F21",
    "statusBar.noFolderBackground": "#1D1F21",
    "statusBar.debuggingBackground": "#2b0000",
    // Tabs
    "editorGroup.border": "#111313",
    "editorGroupHeader.tabsBorder": "#111313",
    "editorGroupHeader.tabsBackground": "#22272B",
    "tab.activeBackground": "#111313",
    "tab.unfocusedActiveBackground": "#111313",
    "tab.border": "#111313",
    "tab.activeBorder": "#55b5db",
    "tab.unfocusedActiveBorder": "#55b5db",
    "tab.inactiveBackground": "#1C2123",
    "tab.inactiveForeground": "#d4d7d6",
    // Git
    "gitDecoration.addedResourceForeground": "#9fca56",
    "gitDecoration.modifiedResourceForeground": "#55b5db",
    "gitDecoration.deletedResourceForeground": "#cd3f45",
    "gitDecoration.untrackedResourceForeground": "#a074c4",
    "gitDecoration.ignoredResourceForeground": "#41535b",
    "gitDecoration.conflictingResourceForeground": "#e6cd69",
    "gitDecoration.submoduleResourceForeground": "#356577",
    // Overview Ruler
    "editorOverviewRuler.bracketMatchForeground": "#d4d7d6",
    "editorOverviewRuler.modifiedForeground": "#55b5db",
    "editorOverviewRuler.addedForeground": "#9fca56",
    "editorOverviewRuler.deletedForeground": "#cd3f45",
    "editorOverviewRuler.errorForeground": "#cd3f45",
    "editorOverviewRuler.warningForeground": "#e6cd69",
    "editorOverviewRuler.infoForeground": "#55b5db",
    "editorOverviewRuler.findMatchForeground": "#d4d7d6",
    // Terminal Colors
    // From base16-seti-ui.minttyrc
    "terminal.background": "#151718",
    "terminal.foreground": "#d4d7d6",
    "terminal.ansiBlack": "#151718",
    "terminal.ansiBrightBlack": "#41535b",
    "terminal.ansiBlue": "#55b5db",
    "terminal.ansiBrightBlue": "#88e8ff",
    "terminal.ansiCyan": "#55dbbe",
    "terminal.ansiBrightCyan": "#88fff1",
    "terminal.ansiGreen": "#9fca56",
    "terminal.ansiBrightGreen": "#d2fd89",
    "terminal.ansiYellow": "#e6cd69",
    "terminal.ansiBrightYellow": "#ffff9c",
    "terminal.ansiMagenta": "#a074c4",
    "terminal.ansiBrightMagenta": "#d3a7f7",
    "terminal.ansiWhite": "#d6d6d6",
    "terminal.ansiBrightWhite": "#ffffff",
    "terminal.ansiRed": "#cd3f45",
    "terminal.ansiBrightRed": "#ff7278",
    // Native Bracket Colors
    "editorBracketHighlight.foreground1": "#e6cd69CC",
    "editorBracketHighlight.foreground2": "#a074c4CC",
    "editorBracketHighlight.foreground3": "#9fca56CC",
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "markup.heading.setext",
        "markup.quote",
        "meta.separator",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#41535b"
      }
    },
    {
      "name": "Blue",
      "scope": [
        "entity.name.function",
        "entity.name.tag.css",
        "entity.name.tag.localname",
        "entity.name.tag.wildcard.css",
        "entity.name.type.namespace",
        "heading",
        "meta.brace.square",
        "meta.function-call",
        "punctuation.definition.array.begin",
        "punctuation.definition.array.end",
        "punctuation.definition.metadata",
        "punctuation.definition.string",
        "punctuation.definition.tag.end",
        "punctuation.definition.tag.end",
        "punctuation.definition.tag",
        "string.other.link.title.markdown.foam",
        "string.quoted.docstring.multi",
        "string.quoted.docstring.single",
        "string.quoted.double",
        "string.quoted.single",
        "support.function",
        "support.type.property-name.json",
        "variable.parameter.function-call"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#55b5db"
      }
    },
    {
      "name": "Green",
      "scope": [
        "entity.other.attribute-name.class",
        "entity.other.attribute-name.id",
        "entity.other.attribute-name.localname",
        "entity.other.attribute-name.pseudo-class",
        "entity.other.attribute-name",
        "entity.other.attribute-name",
        "entity.other.inherited-class",
        "keyword.control.at-rule.import",
        "keyword.control.at-rule.media",
        "keyword.operator.gradients",
        "keyword.operator.logical.and.media",
        "keyword.operator.new",
        "keyword.other.important",
        "keyword.other.unit.px",
        "markup.list.numbered",
        "markup.list.unnumbered",
        "meta.preprocessor",
        "storage.type.class.doxygen",
        "storage.type.class.gtkdoc",
        "variable.language.special.self",
        "variable.other.object.property",
        "variable.other.property",
        "variable.parameter.function.language.special.self"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#9fca56"
      }
    },
    {
      "name": "Darkened Green",
      "scope": [
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag.end",
        "punctuation.definition.typeparameters.begin",
        "punctuation.definition.typeparameters.end",
        "punctuation.parenthesis.close",
        "punctuation.parenthesis.open"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#9fca56CC"
      }
    },
    {
      "name": "Yellow",
      "scope": [
        "keyword.codetag.notation",
        "keyword.control",
        "keyword.fsharp",
        "keyword.operator",
        "keyword.other.async",
        "keyword.other.attribute-specifier",
        "keyword.other.class",
        "keyword.other.delegate",
        "keyword.other.enum",
        "keyword.other.event",
        "keyword.other.get",
        "keyword.other.interface",
        "keyword.other.namespace",
        "keyword.other.new",
        "keyword.other.set",
        "keyword.other.static",
        "keyword.other.struct",
        "keyword.other.typedef",
        "keyword.other.typeof",
        "keyword.other.using",
        "keyword.other.var",
        "keyword.other.where",
        "keyword.symbol.fsharp",
        "keyword.symbol.arrow.fsharp",
        "markup.underline.link",
        "meta.tag.preprocessor.xml",
        "meta.tag.xml",
        "punctuation.definition.list",
        "punctuation.separator.annotation.result.python",
        "punctuation.separator.array.comments",
        "punctuation.separator.dictionary.key-value.comments",
        "punctuation.separator.key-value",
        "punctuation.separator.pointer-access",
        "storage.modifier",
        "storage.type.class",
        "storage.type.function",
        "storage.type"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#e6cd69"
      }
    },
    {
      "name": "Red",
      "scope": [
        "constant.character.escape",
        "constant.language",
        "constant.numeric",
        "constant.other.fsharp",
        "support.constant.font-name",
        "support.constant.media",
        "support.constant.property-values"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#cd3f45"
      }
    },
    {
      "name": "Purple",
      "scope": [
        "entity.name.tag",
        "entity.name.type.class",
        "entity.name.type.struct",
        "keyword.type",
        "markup.inline.raw.string",
        "punctuation.definition.wiki-link",
        "punctuation.section.embedded.begin",
        "punctuation.section.embedded.end",
        "string.other.link.title",
        "support.type.property-name.media",
        "support.type.property-name",
        "variable.other.object"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#a074c4"
      }
    },
    {
      "name": "Bold Blue",
      "scope": [
        "constant.character.format.placeholder.other.python",
        "punctuation.definition.interpolation.begin",
        "punctuation.definition.interpolation.end"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#88e8ff"
      }
    },
    {
      "name": "Font Color",
      "scope": [
        "entity.name.variable.preprocessor.symbol",
        "meta.block",
        "meta.function-call.arguments",
        "punctuation.accessor",
        "punctuation.definition.arguments.begin",
        "punctuation.definition.arguments.end",
        "punctuation.definition.string.begin.json",
        "punctuation.definition.string.end.json",
        "string.quoted.double.json",
        "variable.language"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#d4d7d6"
      }
    },
    {
      "name": "Darkened Font Color",
      "scope": [
        "punctuation.definition.entity",
        "punctuation.terminator.rule",
        "punctuation.terminator.statement",
        "punctuation.terminator"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#888B8A"
      }
    },
    {
      "name": "Bold Font Color",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#d4d7d6"
      }
    },
    {
      "name": "Italic Font Color",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#d4d7d6"
      }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "namespace": {
      "foreground": "#55b5db",
      "fontStyle": "bold"
    },
    "comment": "#41535B",
    "excludedCode": "#41535B",
    "xmlDocCommentComment": "#41535B",
    "xmlDocCommentName": "#9fca56",
    "xmlDocCommentAttributeName": "#9fca56CC",
    "enum": {
      "foreground": "#9fca56",
      "fontStyle": "bold"
    },
    "event": "#9fca56CC",
    "struct": "#a074c4CC",
    "typeParameter": {
      "foreground": "#a074c4CC",
      "fontStyle": "bold"
    },
    "class": "#a074c4",
    "type": "#a074c4",
    "interface": {
      "foreground": "#a074c4",
      "fontStyle": "bold italic"
    },
    "method": "#55b5db",
    "delegate": "#55b5dbCC",
    "string": "#55b5db",
    "number": "#cd3f45",
    "macro": "#cd3f45",
    "enumMember": {
      "foreground": "#cd3f45CC",
      // "fontStyle": ""
    },
    "operator": "#e6cd69",
    "operatorOverloaded": {
      "foreground": "#e6cd69",
      "fontStyle": "bold"
    },
    "memberOperatorOverload": {
      "foreground": "#e6cd69",
      "fontStyle": "bold"
    },
    "controlKeyword": "#e6cd69",
    // "plainKeyword": "#e6cd69",
    "local": "#d4d7d6",
    "variable": "#d4d7d6",
    "variable.static": {
      "foreground": "#d4d7d6",
      "fontStyle": "bold"
    },
    "field": "#d4d7d6",
    "parameter": "#d4d7d6",
    "selfParameter": {
      "foreground": "#9fca56",
      "fontStyle": "italic"
    }
  }
}
